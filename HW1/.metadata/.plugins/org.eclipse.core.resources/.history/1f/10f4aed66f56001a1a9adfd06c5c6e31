import java.util.*;

public class PlayerID {
	private static String playerID;
	public static ArrayList<String> playerTokens = new ArrayList<String>();
	
	List<List> listOfMixedTypes = new ArrayList<List>();

ArrayList<String> listOfStrings = new ArrayList<String>();
ArrayList<Integer> listOfIntegers = new ArrayList<Integer>();

listOfMixedTypes.add(listOfStrings);		
listOfMixedTypes.add(listOfIntegers);
	
	public PlayerID(int numID, String tokenID)
	{
		playerID = tokenID;
	}
	
}
